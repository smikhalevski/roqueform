{
  "name": "Roqueform",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeExternals": true,
  "excludeInternal": true,
  "disableSources": true,
  "hideGenerator": true,
  "readme": "none",
  "visibilityFilters": {
    "protected": true,
    "inherited": true
  },
  "tsconfig": "./tsconfig.json",
  "entryPoints": ["./packages/*/src/main/index.ts", "./packages/roqueform/src/main/plugin/*.ts"],
  "plugin": ["typedoc-plugin-mdn-links"],
  "externalSymbolLinkMappings": {
    "roqueform": {
      "StandardSchemaV1": "https://github.com/standard-schema/standard-schema#readme"
    }
  }
}
